
//a and b are 32 bit integers to add
//op=0 is add, op=1 is subtract
//sum is a 32 bit result of adding/aubtracting a and b
//overflow is a 1 bit cout
module full_adder(a, b, op, sum, overflow);
input [31:0] a, b;
input op;
output [31:0] sum;
output overflow;
wire c0;

c0 = select ? b1'0 : b1'1;




endmodule